#include <stdio.h>
#include <stdlib.h>


int main()
{
    int colb,randb,cola,suma,randa,i,j,A[50][50],B[50][50];
    printf("Matricea A randuri:= ");
    scanf("%d", &randa);
    printf("Matricea A coloane:= ");
    scanf("%d", &cola);

    for(i = 0; i < randa; i++)
    {
        for(j = 0; j < cola; j++)
        {
            printf("A[%d][%d]=", i,j);
            scanf("\n%d", &A[i][j]);
        }
    }

    printf("Matricea B randuri:= ");
    scanf("%d", &randb);
    printf("Matricea B coloane:= ");
    scanf("%d", &colb);

    for(i = 0; i < randb; i++)
    {
        for(j = 0; j < colb; j++)
        {
            printf("B[%d][%d]=", i,j);
            scanf("\n%d", &B[i][j]);
        }
    }

    if(randa!=randb||cola!=colb){
        printf("Matricile sunt diferite");
        return 0;}




    for(i=0; i<randa; i++)
        for(j=0; j<cola; j++)
        {
            if(A[i][j]!=B[i][j])
            {
                printf("Matricile sunt diferite");
                return 0;
            }
        }

    printf("\nMatricile sunt egale!");





    return 0;
}
